{
  "name": "weiran-ui",
  "version": "1.0.4",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "update:install": "node ./build/bin/build-component.install.js",
    "update:routes": "node ./build/bin/build-routes.js",
    "update:components": "node ./build/bin/build-components.json.js",
    "update:src/index": "node ./build/bin/build-entry.js",
    "update:styles-file": "node ./build/bin/build-generate-css.js",
    "update:icons": "node ./build/bin/build-icon-json.js",
    "lint": "vue-cli-service lint",
    "update-all": "npm run update:install & npm run update:routes & npm run update:components &  npm run update:src/index & npm run update:icons & npm run update:styles-file "
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "core-js": "^3.6.5",
    "highlight.js": "^9.18.5",
    "sass": "^1.54.9",
    "sortablejs": "^1.15.0",
    "video.js": "^7.20.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.10.0",
    "vue-router": "^3.6.4"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "@vue/cli-plugin-babel": "~4.5.19",
    "@vue/cli-plugin-eslint": "~4.5.19",
    "@vue/cli-service": "~4.5.19",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "json-templater": "^1.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "transliteration": "^1.1.11",
    "uppercamelcase": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "package.json",
    "README.md",
    "src",
    "packages"
  ],
  "keywords": [
    "ui",
    "vue"
  ],
  "license": "ISC",
  "main": "./src/index.js"
}